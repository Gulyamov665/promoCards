{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main.py"
    },
    {
      "src": "/(.*)",
      "dest": "crud.py"
    },
    {
      "src": "/(.*)",
      "dest": "database.py"
    },
    {
      "src": "/(.*)",
      "dest": "models.py"
    },
    {
      "src": "/(.*)",
      "dest": "database.db"
    },
    {
      "src": "/(.*)",
      "dest": "runtime.txt"
    }
  ]
}
